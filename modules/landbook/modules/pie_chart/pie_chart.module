<?php

function pie_chart_theme($existing, $type, $theme, $path){
  return array(
    'pie-chart'  => array('template' => 'pie-chart','data' => NULL),
  );
}

function pie_chart_page_alter(&$page){
  $library_path = libraries_get_path('lbvis');
  drupal_add_js($library_path . '/js/lbvis.pie.js');
  drupal_add_js(drupal_get_path('module', 'pie_chart').'/main.js');
}