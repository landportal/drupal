<?php

function spider_graph_theme($existing, $type, $theme, $path){
  return array(
    'spider-graph'  => array('template' => 'spider-graph',array('title'=>NULL)),
  );
}

function spider_graph_page_alter(&$page){
  $library_path = libraries_get_path('lbvis');
  drupal_add_js($library_path . '/js/lbvis.spider.js');
  drupal_add_js(drupal_get_path('module', 'spider_graph').'/main.js');
}