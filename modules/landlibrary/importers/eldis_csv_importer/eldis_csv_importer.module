<?php
/**
 * @file
 * Code for eldis importer using JSON.
 */

include_once('eldis_csv_importer.feeds_importer.inc');
include_once('eldis_csv_importer.feeds_tamper.inc');

function eldis_csv_importer_ctools_plugin_api($module = '', $api = '') {
    if ($module == "feeds_tamper" && $api == "feeds_tamper_default") {
        return array("version" => "2");
    }
    if ($module == "feeds" && $api == "feeds_importer_default") {
        return array("version" => "1");
    }
}
